import java.util.Scanner;

/**
 * Created by Tmsnoo on 2017-05-29.
 */
public class Tablice2 {
    public static void main(String[] args) {
int zmienna;
        System.out.println("Podaj który wyraz ciągu liczb Fibo chcesz otrzymać:");
        Scanner wyrazCiągu= new Scanner(System.in);
        zmienna = wyrazCiągu.nextInt();
        int [] fibo = new int[zmienna];
                for(int i=0;i<zmienna;i++) {
                    if (i < 2) {
                        fibo[i] = i;
                    } else
                        fibo[i] = fibo[i - 1] + fibo[i - 2];
                }
        System.out.println(fibo[zmienna-1]);
